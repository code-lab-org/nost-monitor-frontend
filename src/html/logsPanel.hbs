<div
  class="offcanvas offcanvas-end"
  data-bs-scroll="true"
  data-bs-backdrop="false"
  tabindex="-1"
  id="logs"
  aria-labelledby="logsLabel"
>
  <div class="offcanvas-header">
    <h5 class="offcanvas-title" id="logsLabel">Logs</h5>
    <button
      type="button"
      class="btn-close text-reset"
      data-bs-dismiss="offcanvas"
      aria-label="Close"
    ></button>
  </div>
  <div class="offcanvas-body overflow-scroll" id="logsContainer">
  </div>
</div>